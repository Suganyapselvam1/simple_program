using System.IO;
using System;

class Program
{
    static void Main()
    {
        bool z=is_prime_num(11);
        Console.WriteLine(z);
    }
    static bool is_prime_num(int n){
        for(int i=2;i<n;i++){
            if(n%i==0){
                return false;
            }
        }
        return true;
    }
}
